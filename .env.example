# QStash (Phase 8B integration layer)
QSTASH_TOKEN=
QSTASH_CURRENT_SIGNING_KEY=
QSTASH_NEXT_SIGNING_KEY=

# Staging onboarding harness mode (Ticket 8B.4)
# Allowed: stub | real
HARNESS_QSTASH_MODE=stub

# Onboarding scheduler emergency disable flag (Ticket 8B.5)
# Allowed: 0 | 1 | false | true
ONBOARDING_SCHEDULING_DISABLED=0

# Safety inbound intercept tuning (Ticket 11.1)
# Optional; defaults are 10 messages / 60 seconds and 3 strikes.
SAFETY_RATE_LIMIT_MAX_MESSAGES=10
SAFETY_RATE_LIMIT_WINDOW_SECONDS=60
SAFETY_STRIKE_ESCALATION_THRESHOLD=3

# Admin dashboard session signing (Ticket 12.1)
# Must be a high-entropy random value (minimum 32 chars).
ADMIN_SESSION_SECRET=

# Observability (Ticket 13.2)
# Set SENTRY_ENVIRONMENT to local|staging|production.
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=
SENTRY_ENVIRONMENT=local
SENTRY_RELEASE=
